{
  "name": "gitlab-roadmap",
  "version": "0.0.0-development",
  "description": "ðŸ“¢ Create a product roadmap website with GitLab Issue Boards as a backend!",
  "main": "bin/www.js",
  "repository": "Filiosoft/gitlab-roadmap",
  "homepage": "https://filiosoft.org/gitlab-roadmap",
  "bin": {
    "gitlab-roadmap": "./bin/www.js"
  },
  "scripts": {
    "test": "npm run lint && npm audit || true",
    "start": "node bin/www",
    "dev": "DEBUG=glrm:* nodemon bin/www",
    "sls": "sls offline start",
    "lint": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o public/eslint.html",
    "lint:fix": "eslint . --fix",
    "lint!": "npm run format && npm run lint:fix",
    "format": "prettier --write 'lib/*.js' 'bin/*.js' 'config/*.js' '*.js'",
    "cm": "git-cz",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release",
    "build": "webpack --config webpack.config.js",
    "build-release": "webpack --config webpack.config.js --env.prod"
  },
  "author": "Filiosoft Labs <opensource@filiosoft.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "jquery": "^3.3.1",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-gitlab2": "^4.0.0",
    "popper.js": "^1.14.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "serverless-http": "^1.8.0",
    "serverless-offline": "^3.31.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "commitizen": "^3.0.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "debug": "^4.1.0",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.8",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.12.4",
    "serverless": "^1.34.1",
    "serverless-domain-manager": "^2.6.6",
    "style-loader": "^0.19.0",
    "travis-deploy-once": "^5.0.9",
    "webpack": "^3.8.1",
    "webpack-cli": "^3.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint!",
      "git add"
    ]
  },
  "release": {
    "npmPublish": false,
    "tarballDir": "dist",
    "assets": "dist/*.tgz"
  },
  "engines": {
    "node": ">=8"
  }
}
